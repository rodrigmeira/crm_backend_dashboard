generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Project {
  id          String   @id @default(uuid())
  name        String
  description String
  leads       Lead[]
}

model Lead {
  id        String   @id @default(uuid())
  name      String
  email     String
  phone     String
  company   String
  origin    String
  createdAt DateTime @default(now())
  projectId String
  project   Project  @relation(fields: [projectId], references: [id])
}
